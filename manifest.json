{
    "manifest_version": 2,
    "name": "WhatsappWeb 2x",
    "description": "This extension has the propose to incrise playback speed of whatsapp web audios twice",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/"
            ],
            "css": [
                "style.css"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    }

}